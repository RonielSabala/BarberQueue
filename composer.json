{
  "require": {
    "php": ">=8.4.7",
    "vlucas/phpdotenv": "^5.6",
    "phpmailer/phpmailer": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/App/",
      "Config\\": "src/Config/"
    },
    "files": [
      "src/config/RoutesConfig.php"
    ]
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.94"
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix --using-cache=no --quiet --config=.php-cs-fixer.php --path-mode=intersection --allow-risky=yes"
  }
}
